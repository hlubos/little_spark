  .demo{
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    border: 1px solid black;
    background-color: lightgreen;
  }
  .A{
    width: 80px;
  }
  .B{
    width: 100px;
  }
  .C{
    width: 130px;
  }